app_identifier(ENV["MATCH_APP_ID"]) # The bundle identifier of your app
apple_id(ENV["MATCH_APPLE_USERNAME"]) # Your Apple email address

itc_team_id(ENV["MATCH_ITC_TEAM_ID"]) # App Store Connect Team ID
team_id(ENV["MATCH_TEAM_ID"]) # Developer Portal Team ID

